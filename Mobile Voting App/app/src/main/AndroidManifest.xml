<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.futaevoting.evotingapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.androiddeft.loginandregistration.SplashActivity"
            android:screenOrientation="fullSensor">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>

        <activity
            android:name="com.androiddeft.loginandregistration.LoginActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        </activity>
        <activity
            android:name="com.androiddeft.loginandregistration.RegisterActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name="com.androiddeft.loginandregistration.DashboardActivity" />
        <activity android:name="com.androiddeft.loginandregistration.votes" />
        <activity android:name="com.androiddeft.loginandregistration.list" />
        <activity android:name="com.androiddeft.loginandregistration.pres" />
        <activity android:name="com.androiddeft.loginandregistration.gov" />
        <activity android:name="com.androiddeft.loginandregistration.gensec" />
        <activity android:name="com.androiddeft.loginandregistration.asgensec" />
        <activity android:name="com.androiddeft.loginandregistration.finsec" />
        <activity android:name="com.androiddeft.loginandregistration.treasurer" />
        <activity android:name="com.androiddeft.loginandregistration.pro" />
        <activity android:name="com.androiddeft.loginandregistration.dosport" />
        <activity android:name="com.androiddeft.loginandregistration.dosoc" />
        <activity android:name="com.androiddeft.loginandregistration.weloff" />
        <activity android:name="com.androiddeft.loginandregistration.success" />



    </application>

</manifest>